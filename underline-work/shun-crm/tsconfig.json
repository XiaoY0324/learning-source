{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es6",
        "jsx": "react",
        "baseUrl": ".",
        "paths": {
            "@pages/*": ["src/web/pages/*"],
            "@components/*": ["src/web/components/*"],
            "@models/*": ["src/web/models/*"],
            "@assets/*": ["src/web/assets/*"]
        }
    },
    "include": [
        "./src/**/*"
    ]
}